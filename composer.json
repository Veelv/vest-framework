{
    "name": "vest/framework",
    "description": "Framework vest",
    "keywords": ["framework", "vest"],
    "homepage": "https://vest.com",
    "type": "library",
    "license": "MIT",
    "version": "1.0.1",
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-session": "*",
        "ext-tokenizer": "*",
        "fruitcake/php-cors": "^1.3",
        "egulias/email-validator": "^3.2.1|^4.0",
        "composer-runtime-api": "^2.2",
        "psr/container": "^2.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "psr/simple-cache": "^1.0|^2.0|^3.0",
        "firebase/php-jwt": "^6.10",
        "datastax/php-driver": "^1.3"
    },
    "require-dev": {
        "predis/predis": "^2.0.2",
        "phpunit/phpunit": "^10.5|^11.0",
        "fakerphp/faker": "^1.23"
    },
    "support": {
        "issues": "https://github.com/veelv/vest/issues",
        "source": "https://github.com/veelv/vest"
    },
    "authors": [
        {
            "name": "Fabiano Oliveira",
            "email": "oliveira@vest.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vest\\": "src/"
        },
        "files": [
            "src/Exceptions/RouteException.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
